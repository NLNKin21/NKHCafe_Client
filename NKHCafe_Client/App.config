<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	
	<appSettings>
		<!-- Cấu hình Socket Server -->
		<add key="ServerIP" value="127.0.0.1"/>
		<add key="ServerPort" value="12345"/>

		<!-- Cấu hình CSDL (nếu client cần) -->
	</appSettings>

	<connectionStrings>
		<add name="DefaultConnection" connectionString="Data Source=.;Initial Catalog=QLTiemNet;Integrated Security=True"/>
	</connectionStrings>

	
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
</configuration>